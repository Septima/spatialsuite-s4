<?xml version="1.0" encoding="UTF-8"?>
<tool type="plugin">
    <requires>

		<file type="css"    name="[s4.search.version]/css/defaultView.css" />
		<file type="script" name="[s4.search.version]/[s4.search.build]" charset="utf-8"/>
        

        <file type="css"    name="/modules/s4/css/s4.css?_=[s4.version]" />
        
	    [if: isDefinedAndTrue("s4.header.small") ]
	        <file type="css" name="/modules/s4/css/s4SmallHeader.css?_=[s4.version]" />
	    [endif]
	    
	    [if: isDefinedAndTrue("s4.input.topbar") ]
            <file type="css"  name="/modules/s4/css/s4TopBar.css?_=[s4.version]" />
        [endif]
        
        [if: isDefinedAndEquals("s4.color", "green") ]
            <file type="css"    name="[s4.search.version]/css/green.css?_=[s4.version]" />
        [endif]
        
        [if: isDefinedAndEquals("s4.color", "orange") ]
            <file type="css"    name="[s4.search.version]/css/orange.css?_=[s4.version]" />
        [endif]
        
        [if: isDefinedAndEquals("s4.color", "grey") ]
            <file type="css"    name="[s4.search.version]/css/grey.css?_=[s4.version]" />
        [endif]
        
		<file type="script" name="/modules/s4/js/icons.js?_=[s4.version]"/>
		<file type="script" name="/modules/s4/js/s4-control.js?_=[s4.version]"/>
 		<file type="script" name="/modules/s4/js/s4-plugin.js?_=[s4.version]" charset="utf-8"/>
		<file type="script" name="/modules/s4/js/datasourceSearcher.js?_=[s4.version]"/>
    </requires>
    <src/>
    <jssrc/>
</tool>
